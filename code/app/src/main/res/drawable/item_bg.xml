<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- Background color -->
<!--            <solid android:color="#ECD4D4"/>-->

<!--            &lt;!&ndash; Border (stroke) &ndash;&gt;-->
            <stroke
                android:width="2dp"
                android:color="#1E293F"/>

            <corners android:radius="20dp"/>
            <gradient
                android:angle="180"
                android:startColor="#e2f4f8"
                android:endColor="#fdf1f5"
                android:type="linear" />
        </shape>
    </item>
</layer-list>


